    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "objection": "^1.3.0",
